                                        <Ellipse.RenderTransform>
                                            <TranslateTransform/>
                                        </Ellipse.RenderTransform>
                                    </Ellipse>
                                </Border>
                            </StackPanel>
                            <Border x:Name="DeterminateRoot" Background="{TemplateBinding Background}" BorderThickness="4" BorderBrush="{TemplateBinding BorderBrush}" CornerRadius="10" >
                                <Rectangle x:Name="ProgressBarIndicator" Fill="{TemplateBinding Foreground}" HorizontalAlignment="Left" Margin="{TemplateBinding Padding}"/>
                            </Border>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
